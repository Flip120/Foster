#version 300 es

uniform mat4 Matrix;

in vec2 inPosition;
in vec2 inTexCoord;
in vec4 inColor;
in vec4 inType;

out vec2 vTexCoord;
out vec4 vColor;
out vec4 vType;

void main()
{
    vTexCoord = inTexCoord;
    vColor = inColor;
    vType = inType;
    gl_Position = Matrix * vec4(inPosition, 0.0, 1.0);
}