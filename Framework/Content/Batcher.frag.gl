#version 300 es
precision mediump float;
precision highp int;

uniform highp sampler2D Sampler;

in highp vec2 vTexCoord;
in highp vec4 vColor;
in highp vec4 vType;

out highp vec4 FragColor;

void main()
{
    highp vec4 color = texture(Sampler, vTexCoord);
    FragColor = (((color * vType.x) * vColor) + (vColor * (vType.y * color.w))) + (vColor * vType.z);
}